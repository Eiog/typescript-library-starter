{
  "name": "pkg-name",
  "type": "module",
  "version": "0.1.0-beta.0",
  "packageManager": "pnpm@7.18.0",
  "description": "",
  "author": "Eiog",
  "license": "MIT",
  "homepage": "https://github.com/Eiog/tsup-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:Eiog/tsup-starter.git"
  },
  "bugs": "https://github.com/Eiog/tsup-starter/issues",
  "keywords": [
    "tsup-starter"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsc --noEmit && tsup",
    "unbuild": "tsc --noEmit && unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "update:deps": "taze -w && pnpm i",
    "type:check": "tsc --noEmit",
    "cz": "czg",
    "commit": "git pull && git add -A && pnpm cz && git push",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks"
  },
  "devDependencies": {
    "@eiog/eslint-config-ts": "^0.0.2",
    "@eiog/ts-config": "^0.0.3",
    "@vitest/ui": "^0.34.4",
    "bumpp": "^9.2.0",
    "commitlint": "^17.7.1",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "eslint": "^8.49.0",
    "esno": "^0.17.0",
    "lint-staged": "^14.0.1",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "tsup": "7.2.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm type:check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix"
    ]
  }
}
